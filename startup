if term.isColor() and term.isColor then
tData["path"]=shell.getRunningProgram():sub(1,-8).."/"
tPath=tData["path"]
shell.run(tData["path"].."System/Boot.lua")
shell.run(tData["path"].."System/Lock.lua")
term.setTextColor(colors.yellow)
print("rOS has crashed!")
else
print("rOS is only for Advanced Pocket Computers, but you can run it also on Advanced Computers")
end